{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-redis": "*",
        "ext-intl": "*",
        "laravel/framework": "^v12.4.1",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "league/flysystem-aws-s3-v3": "^3.29",
        "matomo/device-detector": "^6.4",
        "propaganistas/laravel-disposable-email": "^2.4",
        "spatie/laravel-permission": "^6.9",
        "stevebauman/location": "^7.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "driftingly/rector-laravel": "^2.0.2",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^v3.2.0",
        "laravel/boost": "^1.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.26",
        "laravel/tinker": "^2.10",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-drift": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-profanity": "^4.1",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpmd/phpmd": "^2.15",
        "phpro/grumphp": "^2.8",
        "phpstan/phpstan-strict-rules": "^2.0.4",
        "rector/rector": "^2.0.10",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Scripts\\ComposerScripts::devModeOnly",
            "@php ./vendor/bin/grumphp git:init",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "test": [
            "vendor/bin/pest --log-junit storage/testing/tests.xml --testdox --display-notices"
        ],
        "parallel-test": [
            "@php artisan test --parallel --recreate-databases"
        ],
        "lint": [
            "./vendor/bin/pint"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "php-md": [
            "./vendor/bin/phpmd app,config,database,routes ansi ./phpmd-ruleset.xml"
        ],
        "rector": [
            "./vendor/bin/rector"
        ],
        "type-coverage": [
            "./vendor/bin/pest --type-coverage --compact"
        ],
        "profanity-check": [
            "/vendor/bin/pest --profanity"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
